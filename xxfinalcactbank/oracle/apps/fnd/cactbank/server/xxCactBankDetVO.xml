   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="xxfinalcactbank.oracle.apps.fnd.cactbank.server.xxCactBankDetVORowImpl"
   ComponentClass="xxfinalcactbank.oracle.apps.fnd.cactbank.server.xxCactBankDetVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  cust.user_id ,
        bnk.ACC_NO,
         cust.FIRST_NAME || ' ' || cust.MIDDLE_NAME || ', ' || cust.LAST_NAME
            AS ACCOUNT_holder_name,
         cust.Branch,
         cust.EMAIL_ID,
         cust.MOBILE_NO,
         bnk.BALANCE
  FROM   xxcact_CUSTOMER cust, xxcact_BANK bnk
WHERE   cust.user_id = bnk.user_id
   ]]></SQLQuery>
